<% layout('layout', {title:'Kontakter'}) %>
<h2>Kontakter</h2>
<table border="1" cellpadding="8" cellspacing="0">
  <thead><tr><th>ID</th><th>Namn</th><th>Email</th><th>FÃ¶retag</th><th>Bransch</th><th>Meddelande</th><th>Datum</th></tr></thead>
  <tbody>
    <% contacts.forEach(c => { %>
      <tr>
        <td><%= c.id %></td>
        <td><%= c.name %></td>
        <td><%= c.email %></td>
        <td><%= c.company %></td>
        <td><%= c.industry %> <%= c.otherIndustry ? '('+c.otherIndustry+')' : '' %></td>
        <td><%= c.message %></td>
        <td><%= c.createdAt %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
